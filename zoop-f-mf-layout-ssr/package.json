    "axios": "0.16.2",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.6.0",
    "compression": "1.7.4",
    "concurrently": "6.0.0",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "express-http-proxy": "1.6.2",
    "forgo-lazy": "^1.1.1",
    "lodash": "4.17.21",
    "node-polyfill-webpack-plugin": "^1.0.3",
    "nodemon": "^2.0.7",
