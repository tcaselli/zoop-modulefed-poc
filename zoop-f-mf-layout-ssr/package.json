    "@types/react-router-dom": "^5.1.6"
  },
  "scripts": {
    "generate-imported-component": "imported-components src src/imported.ts",
    "start:frontend": "concurrently \"yarn start:server\" \"cd ../zoop-f-mf-app1 && node dist/server.js\" \"cd ../zoop-f-mf-app2 && node dist/server.js\" \"cd ../zoop-f-mf-app4-ssr && yarn start:server\"",
    "start:backend": "cd ../zoop-b-ms-app && mvn spring-boot:run",
    "install:deps": "concurrently \"cd ../zoop-f-mf-app1 && yarn\" \"cd ../zoop-f-mf-app2 && yarn\" \"cd ../zoop-f-mf-app4-ssr && yarn\"  \"cd ../zoop-b-ms-app && mvn install\"",
    "build:frontend": "concurrently \"yarn build\"  \"cd ../zoop-f-mf-app1 && yarn build\" \"cd ../zoop-f-mf-app2 && yarn build\" \"cd ../zoop-f-mf-app4-ssr && yarn build\"",
    "build": "shop-script build",
    "start:server": "nodemon dist/main.node.bundle.js 80",
    "bundle-analysis": "shop-script bundle-analysis",
    "static-analysis": "shop-script static-analysis",
    "static-analysis:fix": "shop-script static-analysis:fix",
    "jest": "shop-script jest",
    "jest:once": "shop-script jest:once",
    "cypress:open": "shop-script cypress:open",
    "cypress:run": "shop-script cypress:run",
    "cypress:install": "shop-script cypress:install"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/@com.zooplus/zoop-f-config/config/eslintrc"
    ]
  },
  "eslintIgnore": [
    "config",
    "/dist",
    "/node_modules",
    "/server",
    ".*.js",
    "*.js"
  ],
  "babel": {
    "presets": [
      "./node_modules/@com.zooplus/zoop-f-config/config/babelrc"
    ],
    "plugins": [
      "react-imported-component/babel"
    ]
  },
  "husky": {
    "hooks": {}
  },
  "prettier": "./node_modules/@com.zooplus/zoop-f-config/config/prettierrc",
  "browserslist": [
    "extends @com.zooplus/zoop-f-config/config/browserslistrc"
  ]
}
